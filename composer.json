{
    "name": "lotgd/crate-sample",
    "description": "Sample crate to demonstrate how to wrap the core.",
    "license": "AGPL-3.0",
    "type": "lotgd-crate",
    "autoload": {
        "psr-4": {
            "LotGD\\Crates\\Sample\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://code.lot.gd"
        },
        {
            "type": "path",
            "url": "../module-scene-editor"
        },
        {
            "type": "vcs",
            "url": "https://github.com/austenmc/tree-printer"
        }
    ],
    "require": {
        "lotgd/core": "0.1.0-alpha",
        "lotgd/module-scene-editor": "dev-master"
    },
    "config": {
        "secure-http": false
    }
}
